{
  "name": "JavaScript Bots Showcase",
  "version": "0.1",
  "description": "A variety of JavaScript Bots and Bookmarklets to demonstrate javascript console automation",
  "background": {
    "persistent": true,
    "scripts": [
                "js/menu.js",
                "js/background.js"
              ]              
  },
  "web_accessible_resources": [
    "js/*"
  ],
  "permissions": [
    "contextMenus",
    "activeTab"
    ],
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
    },
  "manifest_version": 2
}
